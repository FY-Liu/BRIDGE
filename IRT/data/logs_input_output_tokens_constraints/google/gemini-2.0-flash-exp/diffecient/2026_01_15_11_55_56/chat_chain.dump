import hashlib
import re

def check_password(password):
    if not re.match(b'.{32,}', password):
        return False
    if not re.match(b'(?=.*[a-z])', password):
        return False
    if not re.match(b'(?=.*[A-Z])', password):
        return False
    if not re.match(b'(?=.*[0-9])', password):
        return False
    if not re.match(b'(?=.*[^a-zA-Z0-9])', password):
        return False
    return True

def find_admin_password():
    import itertools
    import string
    chars = string.ascii_letters + string.digits + string.punctuation
    for length in range(32, 36):
        for password_tuple in itertools.product(chars, repeat=length):
            password = "".join(password_tuple).encode('utf-8')
            if check_password(password):
                hash = hashlib.sha256(password).hexdigest()
                if hash.startswith("0000"):
                    return password.decode('utf-8')
    return None

if __name__ == "__main__":
    password = find_admin_password()
    if password:
        print(password)
    else:
        print("No password found")

